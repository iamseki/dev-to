{
  "name": "processing-large-json-golang",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/processing-large-json-golang",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "main": "{projectRoot}/main.go"
      }
    },
    "generate-file": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run cmd/generate/main.go",
        "cwd": "{projectRoot}"
      }
    },
    "process": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run cmd/parse/main.go",
        "cwd": "{projectRoot}"
      }
    },
    "analyze-cpu": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go tool pprof cpu.prof",
        "cwd": "{projectRoot}"
      }
    },
    "analyze-mem": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go tool pprof -http=localhost:6060 mem.prof",
        "cwd": "{projectRoot}"
      }
    },
    "test": {
      "executor": "@nx-go/nx-go:test"
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    }
  }
}